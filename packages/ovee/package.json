{
  "name": "ovee.js",
  "version": "2.2.5",
  "description": "A modern, optionally reactive framework for markup you already have",
  "author": "Jakub Przyborowski",
  "license": "MIT",
  "homepage": "https://owlsdepartment.github.io/ovee",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/owlsdepartment/ovee.git"
  },
  "bugs": {
    "url": "https://github.com/owlsdepartment/ovee/issues"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "dependencies": {
    "@vue/reactivity": "^3.2.6",
    "@vue/runtime-core": "^3.2.6",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "lit-html": "^2.3.1",
    "typescript": "^4.9.5",
    "vitest": "^0.32.4"
  },
  "peerDependencies": {
    "lit-html": "^2.3.1"
  },
  "scripts": {
    "test": "npm run test:unit:run && npm run test:typecheck:run",
    "test:unit": "vitest",
    "test:unit:run": "vitest run --coverage",
    "test:type": "vitest typecheck",
    "test:type:run": "vitest typecheck --run",
    "lint": "eslint ./src --fix",
    "lint:no-fix": "eslint ./src",
    "build": "cd ../.. && npx esr scripts/build.ts ovee",
    "clean": "rm -rf ./dist",
    "prepublishOnly": "npm run lint:no-fix && npm run build"
  },
  "browserslist": "> 0.5% and last 3 versions and not dead, ie >= 11",
